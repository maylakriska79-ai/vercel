{
  "functions": {
    "api/bun.ts": {
      "runtime": "@vercel/bun"
    }
  },
  "probes": [
    {
      "path": "/api/bun",
      "mustContain": "\"runtime\":\"bun\"",
      "responseHeaders": {
        "x-got-middleware": "true"
      }
    },
    {
      "path": "/api/bun-2",
      "mustContain": "\"runtime\":\"bun\"",
      "responseHeaders": {
        "x-got-middleware": "true"
      }
    },
    {
      "path": "/api/node",
      "mustContain": "\"runtime\":\"node\"",
      "responseHeaders": {
        "x-got-middleware": "true"
      }
    },
    {
      "path": "/non-exist",
      "mustContain": "RANDOMNESS_PLACEHOLDER:middleware:bun",
      "responseHeaders": {
        "x-got-middleware": "true"
      }
    }
  ]
}
